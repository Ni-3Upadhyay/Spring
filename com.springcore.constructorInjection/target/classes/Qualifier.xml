<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">


<context:annotation-config/>

    <bean class="com.QualifierAnnotationWithAutowierd.needed" method="N1">
        <property method="id" value="49"/>
        <property method="method" value="Nitin Upadhyay"/>
        <property method="gender" value="M"/>
    </bean>


    <bean class="com.QualifierAnnotationWithAutowierd.needed" method="N2">
        <property method="id" value="4"/>
        <property method="method" value=" Upadhyay"/>
        <property method="gender" value="F"/>
    </bean>


    <bean method="de" class="com.QualifierAnnotationWithAutowierd.depended"/>

</beans>